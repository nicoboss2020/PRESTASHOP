CREATE TABLE `client` (
  `id_client` int(11) NOT NULL,
  `prenom` varchar(45) DEFAULT NULL,
  `nom` varchar(45) DEFAULT NULL,
  `date_inscription` date DEFAULT NULL,
  `pays` varchar(45) DEFAULT NULL,
  `ville` varchar(45) DEFAULT NULL,
  `cp` int(11) DEFAULT NULL,
  `sexe` varchar(45) DEFAULT NULL,
  `newsletter` int(11) DEFAULT NULL,
  `date_inscription_newsletter` date DEFAULT NULL,
  `ddn` date DEFAULT NULL,
  PRIMARY KEY (`id_client`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


CREATE TABLE `dw_totallydata`.`commande` (
  `id_detail_commande` INT NOT NULL,
  `quantite_produit` INT NULL,
  `prix_unitaire_taxes_incluses` DECIMAL NULL,
  `pourcentage_reduction` DECIMAL NULL,
  `date_commande` DATETIME NULL,
  PRIMARY KEY (`id_detail_commande`));


CREATE TABLE `groupe_categorie` (
  `id_categorie` int(11) NOT NULL,
  `id_produit` int(11) DEFAULT NULL,
  `position_produit` int(11) DEFAULT NULL,
  `id_parent` int(11) DEFAULT NULL,
  `level_depth` int(11) DEFAULT NULL,
  `position` int(11) DEFAULT NULL,
  `id_lang` int(11) DEFAULT NULL,
  `nom_categorie` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`id_categorie`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


CREATE TABLE `produit` (
  `id_produit` int(11) NOT NULL,
  `En_vente` int(11) DEFAULT NULL,
  `Pas_en_stock` int(11) DEFAULT NULL,
  `Quantite` int(11) DEFAULT NULL,
  `Prix` float DEFAULT NULL,
  `Nom_produit` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`id_produit`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


CREATE TABLE `calendrier` (
  `id_temps` datetime NOT NULL,
  `annee` int(11) DEFAULT NULL,
  `mois` varchar(45) DEFAULT NULL,
  `jour` int(11) DEFAULT NULL,
  `flag_commande` int(11) DEFAULT NULL,
  PRIMARY KEY (`id_temps`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

